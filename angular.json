{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "modules",
  "projects": {
    "common": {
      "projectType": "library",
      "root": "modules/common",
      "sourceRoot": "modules/common/src",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "modules/common/tsconfig.lib.json",
            "project": "modules/common/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "modules/common/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["modules/common/src/**/*.ts", "modules/common/src/**/*.html"]
          }
        }
      }
    },
    "auth": {
      "projectType": "library",
      "root": "modules/auth",
      "sourceRoot": "modules/auth/src",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "modules/auth/tsconfig.lib.json",
            "project": "modules/auth/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "modules/auth/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["modules/auth/src/**/*.ts", "modules/auth/src/**/*.html"]
          }
        }
      }
    },
    "agenda": {
      "projectType": "library",
      "root": "modules/agenda",
      "sourceRoot": "modules/agenda/src",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "modules/agenda/tsconfig.lib.json",
            "project": "modules/agenda/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "modules/agenda/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["modules/agenda/src/**/*.ts", "modules/agenda/src/**/*.html"]
          }
        }
      }
    },
    "map": {
      "projectType": "library",
      "root": "modules/map",
      "sourceRoot": "modules/map/src",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "modules/map/tsconfig.lib.json",
            "project": "modules/map/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "modules/map/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["modules/map/src/**/*.ts", "modules/map/src/**/*.html"]
          }
        }
      }
    },
    "teams": {
      "projectType": "library",
      "root": "modules/teams",
      "sourceRoot": "modules/teams/src",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "modules/teams/tsconfig.lib.json",
            "project": "modules/teams/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "modules/teams/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["modules/teams/src/**/*.ts", "modules/teams/src/**/*.html"]
          }
        }
      }
    }
  },
  "defaultProject": "common"
}
