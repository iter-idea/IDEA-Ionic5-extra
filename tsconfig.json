{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "agenda": ["dist/agenda"],
      "auth": ["dist/auth"],
      "common": ["dist/common"],
      "map": ["dist/map"],
      "plans-subscription": ["dist/plans-subscription"],
      "speechRecognition": ["dist/speech-recognition"],
      "teams": ["dist/teams"]
    },
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,

    "declaration": true,
    "declarationMap": true,
    "inlineSources": true,
    "importHelpers": true,
    "module": "commonjs",
    "removeComments": true,
    "sourceMap": true,
    "target": "es6",
    "strict": true,
    "strictPropertyInitialization": false,
    "strictNullChecks": false,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true
  },
  "angularCompilerOptions": {
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true,
    "enableIvy": false
  }
}
